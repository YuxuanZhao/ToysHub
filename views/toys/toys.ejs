<%- include ('../partials/header') %>

<% for (i = 0; i < toys.length; i++) { %>
    <div class = "col-md-3 col-sm-6">
        <div class="thumbnail">
            <img src = "<%= toys[i].picture %>">
            <div class="caption">
                <p><%= toys[i].name%></p>
            </div>
            <p>
                <a href = "/toys/<%= toys[i]._id %>" class = "btn btn-primary">More Info</a>    
            </p>
        </div>
    </div>
<% } %>

<%- include ('../partials/footer') %>